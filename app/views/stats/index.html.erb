<h3>Počty registrací dle krajů</h3>
<table>
	<thead><tr><th>Kraj</th><th>Počet registrací</th></tr></thead>
	<tbody>
		<% for region, commisaries in @stats%>
		<tr><td><%= region.name %></td><td><%= commisaries.count %></td></tr>
		<% end %>
		<tr><th>Celkem osob</th><th><%= Commisary.count %></th></tr>
		<tr><th>Celkem radnic</th><th><%= TownHall.joins(:commisaries).uniq.size %></th></tr>
	</tbody>
</table>
