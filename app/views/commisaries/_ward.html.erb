<tr><th>Kraj: </th><td><%=@commisary.region.name%></td></tr>
<tr><th>Obec: </th><td><span class="pull-right"><%= image_tag(@commisary.ward.town_hall.heraldry_path, width: '50px') %></span><%=@commisary.municipality.name%></td></tr>
<% if @commisary.ward.district %>
<tr><th>Městská část / obvod:</th><td><%= @commisary.ward.district.name %></td></tr>
<% end %>
<% if @commisary.ward.town_hall.address %>
<tr><th>Adresa úřadu:</th><td><%=@commisary.ward.town_hall.address.gsub("\n",', ')%></td></tr>
<% end %>
<tr><th>Okrsek: </th><td><%=@commisary.ward.external_id%></td></tr>
